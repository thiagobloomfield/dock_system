{
  "name": "teste_dev_dock",
  "version": "1.0.0",
  "description": "Teste Dock",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "prod": "npm run build && node dist/entry.js",
    "test": "jest --no-cache",
    "dev": "nodemon --watch \"src\" --exec \"ts-node src/entry.ts\" -e ts"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiagobloomfield/teste_dev_dock.git"
  },
  "author": "Thiago Bloomfield",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thiagobloomfield/teste_dev_dock/issues"
  },
  "homepage": "https://github.com/thiagobloomfield/teste_dev_dock#readme",
  "dependencies": {
    "express": "^4.17.1",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/supertest": "^2.0.10"
  }
}
